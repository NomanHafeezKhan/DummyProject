@model IEnumerable<WebApplication1.Models.ModelContext>

@{
    ViewBag.Title = "Listcategory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (ViewBag.Message != null)
{
    <script type="text/javascript">

            window.onload = function () {
                alert("@ViewBag.Message");
            };
    </script>
}
<h2>Listcategory</h2>

<p>
    @Html.ActionLink("Create New", "Addcategory")
</p>
<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        @*<th>
            @Html.DisplayNameFor(model => model.EntryDate)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th></th>
    </tr>
    @{
        int sno = 0;
    }

    @foreach (var item in Model)
    {
<tr>
    <td>
        @{ sno++; }
        @sno
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    @*<td>
            @Html.DisplayFor(modelItem => item.EntryDate)
        </td>*@
    <td>
        @Html.DisplayFor(modelItem => item.Age)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>
    <td>
        @Html.ActionLink("Edit", "Addcategory", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
    }

</table>

<script src="~/Scripts/mytypescript.js"></script>
<script>



</script>